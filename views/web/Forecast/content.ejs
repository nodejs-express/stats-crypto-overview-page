                        <div class="title">Market Cap.</div>
                        <div class="amount"><%= Numeric.setCurrency(data.mcap);%></div>
                      </div>
                      <div class="invest-data-history">
                        <div class="title">Total Volume</div>
                        <div class="amount"><%= Numeric.setCurrency(parseFloat(data.volume));%></div>
                      </div>
                    </div>
                    <div class="invest-data-ck"></div>
